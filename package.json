{
  "name": "rag-template-starter",
  "version": "1.0.0",
  "description": "Production-ready RAG template for GDPR-compliant AI applications",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch --import ./src/lib/dotenv.ts src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "postinstall": "prisma generate"
  },
  "keywords": [
    "rag",
    "azure",
    "openai",
    "typescript",
    "gdpr"
  ],
  "author": "Jacob Lazda",
  "license": "MIT",
  "packageManager": "pnpm@10.29.1",
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/express": "^5.0.6",
    "@types/ioredis": "^5.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.2.2",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitest/ui": "^4.0.18",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.8.1",
    "prisma": "^7.3.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@azure/ai-form-recognizer": "^5.1.0",
    "@azure/identity": "^4.13.0",
    "@azure/openai": "^2.0.0",
    "@azure/search-documents": "^12.2.0",
    "@azure/storage-blob": "^12.30.0",
    "@prisma/client": "^7.3.0",
    "bullmq": "^5.67.3",
    "dotenv": "^17.2.4",
    "express": "^5.2.1",
    "ioredis": "^5.9.2",
    "multer": "^2.0.2",
    "openai": "^6.18.0",
    "pdf-parse": "^2.4.5",
    "zod": "^4.3.6"
  }
}
